{
  "name": "@ethlimo/ens-hooks",
  "version": "0.2.1",
  "description": "experimental ENS query environment for ENS hooks draft ENSIPs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethlimo/ens-hooks.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
